    "eslint-config-prettier": "^9.1.0",
    "gray-matter": "^4.0.3",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "next-sitemap": "^4.2.3",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react-syntax-highlighter": "^15.5.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix",
      "prettier --write --ignore-unknown"
